{
  "name": "EpicSevenPlanner",
  "homepage": "https://arkynized.herokuapp.com/",
  "version": "1.0.0",
  "description": "A planner tool for Epic Seven users to organize their resources.",
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.8"
  },
  "main": "./server/dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --project ./server/tsconfig.json",
    "prestart": "npm run build",
    "postinstall": "npm run build",
    "start": "node ./server/dist/index.js",
    "server": "nodemon --config ./server/nodemon.json",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-resolvers": "2.2.1",
    "@types/express": "^4.17.13",
    "@types/fuzzy-search": "^2.1.1",
    "@types/node": "^16.9.0",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "prisma": "^3.2.1"
  },
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "apollo-server-express": "^3.3.0",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fuzzy": "^0.1.3",
    "graphql": "^15.5.3",
    "typescript": "^4.5.2"
  }
}
